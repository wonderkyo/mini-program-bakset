/* components/myCard.wxss */
.card{
  margin: 20rpx;
  padding: 20rpx;
  border-radius: 12rpx;
  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.1); /* 设置阴影 */
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.mycard-flex{
  display: flex;
  align-items: flex-start;
}
.mycard-flex-center{
  display: flex;
  align-items: center;
}
